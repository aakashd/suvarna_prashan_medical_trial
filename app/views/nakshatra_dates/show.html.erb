<%- model_class = NakshatraDate -%>
<div class="page-header">
  <h1><%=t '.title', :default => NakshatraDate %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nakshatra_date) %></dt>
      <dd><pre class="prettyprint"><%= @nakshatra_date.nakshatra_date %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              nakshatra_dates_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_nakshatra_date_path(@nakshatra_date), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              nakshatra_date_path(@nakshatra_date),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

